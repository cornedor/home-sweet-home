{
  "manifest_version": 2,
  "name": "Home! Sweet Home!",
  "version": "1.0.2",
  "description": "A personal new tab page",
  "author": "corne",
  "homepage_url": "https://www.corne.info/",
  "default_locale": "nl",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "homepage@corne.info"
    }
  },
  "content_security_policy": "script-src 'self' https://ga.jspm.io blob: 'sha256-1jAV6CdWkLL96+6nbSmW+evEGns1ecFnXBPz+e98aqQ='; object-src 'self'",
  "permissions": [
    "storage",
    "search",
    "idle",
    "https://*/*"
  ],
  "optional_permissions": [
    "<all_urls>",
    "bookmarks",
    "topSites"
  ],
  "options_ui": {
    "page": "options.html"
  }
}
